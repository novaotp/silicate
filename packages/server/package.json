  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "npm run build && pm2 start .\\build\\server\\src\\app.js --watch --no-vizion && npm run build:watch",
    "dev:monitor": "pm2 monit --name app",
    "dev:kill": "pm2 kill --name app",
    "build": "tsc && tsc-alias",
    "build:watch": "concurrently --kill-others \"tsc -w\" \"tsc-alias -w\"",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "generate:jwt": "node -e \"console.log(require('crypto').randomBytes(128).toString('hex'))\"",
    "prepare": "ts-patch install && typia patch"
  },
  "license": "ISC",
