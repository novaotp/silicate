    "jose": "^5.2.3",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.4",
    "socket.io": "^4.7.5",
    "swagger-ui-dist": "^5.17.14",
    "swagger-ui-express": "^5.0.1",
    "typia": "^6.6.1"
  },
  "devDependencies": {
